<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="296.0" prefWidth="314.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.producersapp.PrimaryController">
   <children>
      <Label text="Question" />
      <TextField fx:id="questionField" />
      <Label text="Answers" />
      <TextField fx:id="answersField" />
      <Label text="Day" />
      <TextField fx:id="datePicker" />
      <Button fx:id="primaryButton" onAction="#submitNewSurvey" text="Submit" />
      <TextArea fx:id="message" prefHeight="200.0" prefWidth="200.0" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
